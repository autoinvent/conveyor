{
  "name": "@autoinvent/conveyor",
  "version": "1.0.0-beta.2",
  "description": "GraphiQL for magql",
  "license": "BlueOak-1.0.0",
  "author": "Moebius Solutions",
  "files": ["dist"],
  "main": "./dist/conveyor.umd.js",
  "module": "./dist/conveyor.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/conveyor.mjs",
      "require": "./dist/conveyor.umd.js"
    },
    "./dist/styles/index.css": {
      "import": "./dist/styles/index.css",
      "require": "./dist/styles/index.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "rome format . --write",
    "run-tsc": "tsc",
    "run-rome-lint": "rome check . --apply",
    "run-stylelint": "stylelint --cache --fix --ignore-path .gitignore '**/*.css'",
    "lint": "run-p run-tsc run-stylelint run-rome-lint",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "validate": "run-p lint test:ci"
  },
  "dependencies": {
    "@babel/core": "^7.22.9",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0",
    "bootswatch": "^5.3.0",
    "react-bootstrap": "^2.8.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.45.2",
    "react-icons": "^4.10.1",
    "react-select": "^5.7.4",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.32.0",
    "@tanstack/react-query-devtools": "^4.32.0",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/react-helmet": "6.1.6",
    "@types/testing-library__jest-dom": "5.14.8",
    "@vitejs/plugin-react": "4.0.3",
    "@vitest/ui": "0.33.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rome": "12.1.3",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.12",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-node-polyfills": "^0.11.2",
    "vitest": "0.33.0"
  },
  "lint-staged": {
    "*": "rome format . --write",
    "*.css": "stylelint --cache --fix",
    "*.{ts,tsx}": [
      "rome check --apply",
      "vitest related --run --coverage=false"
    ]
  }
}
