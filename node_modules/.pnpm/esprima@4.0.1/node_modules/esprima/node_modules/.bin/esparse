#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/esprima@4.0.1/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/esprima@4.0.1/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esparse.js" "$@"
else
  exec node  "$basedir/../../bin/esparse.js" "$@"
fi
