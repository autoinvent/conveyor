#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint@8.18.0/node_modules/eslint/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint@8.18.0/node_modules/eslint/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint@8.18.0/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint-config-airbnb-typescript@17.0.0_e35d7599b49a2d57b7a29a765b615088/node_modules/eslint-config-airbnb-typescript/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint-config-airbnb-typescript@17.0.0_e35d7599b49a2d57b7a29a765b615088/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint@8.18.0/node_modules/eslint/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint@8.18.0/node_modules/eslint/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint@8.18.0/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint-config-airbnb-typescript@17.0.0_e35d7599b49a2d57b7a29a765b615088/node_modules/eslint-config-airbnb-typescript/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/eslint-config-airbnb-typescript@17.0.0_e35d7599b49a2d57b7a29a765b615088/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@8.18.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@8.18.0/node_modules/eslint/bin/eslint.js" "$@"
fi
